<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="QuizGame.Views.CodeSnippetView">
    
    <WebView IsVisible="{Binding IsVisible}" WidthRequest="{Binding Width}" HeightRequest="{Binding Height}">
        <WebView.Source>
            <HtmlWebViewSource Html="{Binding Html2Display}"/>
        </WebView.Source>
    </WebView>
</ContentView>
